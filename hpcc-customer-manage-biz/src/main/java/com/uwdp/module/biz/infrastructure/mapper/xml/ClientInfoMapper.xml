<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uwdp.module.biz.infrastructure.mapper.ClientInfoMapper">
    <select id="getClientInfoCount" resultType="java.lang.Integer">
        SELECT COUNT(*) FROM t_clientInfo WHERE  fdName=#{fdName} and fdUnit=#{fdUnit}
    </select>
</mapper>
